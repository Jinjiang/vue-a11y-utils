{
  "name": "vue-a11y-utils",
  "version": "0.7.0",
  "description": "Utilities for accessibility (a11y) in Vue.js",
  "type": "module",
  "main": "./dist/vue-a11y-utils.umd.js",
  "module": "./dist/vue-a11y-utils.mjs",
  "exports": {
    ".": {
      "import": "./dist/vue-a11y-utils.mjs",
      "require": "./dist/vue-a11y-utils.umd.js"
    }
  },
  "types": "types/index.d.ts",
  "files": [
    "dist/*",
    "README.md",
    "types",
    "src"
  ],
  "scripts": {
    "dev": "vite",
    "build": "vue-tsc --noEmit && vite build --config vite.config.lib.ts",
    "build:force": "vite build --config vite.config.lib.ts",
    "lint": "vue-tsc --noEmit",
    "format": "prettier --write .{,/examples,/src}/*.{js,json,css,md,ts,vue}",
    "test": "vitest",
    "docs": "vuepress dev docs-src",
    "docs:build": "vuepress build docs-src -d docs"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jinjiang/vue-a11y-utils.git"
  },
  "keywords": [
    "a11y",
    "aria",
    "vue",
    "vue.js"
  ],
  "author": "Jinjiang <zhaojinjiang@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jinjiang/vue-a11y-utils/issues"
  },
  "homepage": "https://github.com/Jinjiang/vue-a11y-utils#readme",
  "devDependencies": {
    "@vue/test-utils": "1.0.0-beta.25",
    "husky": "^3.0.5",
    "prettier": "^1.18.2",
    "pretty-quick": "^1.11.1",
    "typescript": "^4.8.4",
    "vite": "^3.0.9",
    "vite-plugin-vue2": "^2.0.2",
    "vitest": "^0.24.3",
    "vue": "^2.7.10",
    "vue-template-compiler": "^2.7.10",
    "vue-tsc": "^1.0.7",
    "vuepress": "^1.9.7"
  },
  "dependencies": {
    "vue-class-component": "^6.3.2"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
