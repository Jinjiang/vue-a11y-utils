{
  "name": "vue-a11y-utils",
  "version": "0.1.0",
  "description": "Utilities for accessibility (a11y) in Vue.js",
  "main": "./lib/index.js",
  "files": [
    "dist",
    "lib"
  ],
  "scripts": {
    "build:parcel": "parcel build src/index.ts -d dist -o vue-a11y.js",
    "build:lib": "tsc --build src/tsconfig.json",
    "build": "npm run build:parcel && npm run build:lib",
    "examples": "parcel examples/index.html --public-url /examples/",
    "examples:build": "parcel build examples/index.html --public-url /examples/ -d ./.vuepress/public/examples",
    "docs": "vuepress dev",
    "docs:build": "npm run examples:build && vuepress build",
    "lint": "prettier --write .{,/examples,/src}/*.{js,json,css,md,ts,vue}",
    "test": "jest"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Jinjiang/vue-a11y-utils.git"
  },
  "keywords": [
    "a11y",
    "aria",
    "vue",
    "vue.js"
  ],
  "author": "Jinjiang <zhaojinjiang@me.com>",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/Jinjiang/vue-a11y-utils/issues"
  },
  "homepage": "https://github.com/Jinjiang/vue-a11y-utils#readme",
  "devDependencies": {
    "@types/jest": "^23.3.5",
    "@vue/test-utils": "^1.0.0-beta.25",
    "babel-core": "^6.26.3",
    "babel-jest": "^23.6.0",
    "babel-plugin-transform-decorators-legacy": "^1.3.5",
    "babel-preset-env": "^1.7.0",
    "husky": "^1.1.2",
    "jest": "^23.6.0",
    "parcel-bundler": "^1.10.3",
    "prettier": "^1.14.3",
    "pretty-quick": "^1.8.0",
    "ts-jest": "^23.10.4",
    "typescript": "^3.1.3",
    "vue": "^2.5.17",
    "vue-class-component": "^6.3.2",
    "vue-jest": "^3.0.0",
    "vue-template-compiler": "^2.5.17",
    "vuepress": "^0.14.4"
  },
  "jest": {
    "moduleFileExtensions": [
      "js",
      "ts",
      "json",
      "vue"
    ],
    "transform": {
      ".*\\.(vue)$": "vue-jest",
      "^.+\\.tsx?$": "ts-jest"
    },
    "testRegex": "(/__tests__/.*|(\\.|/)(test|spec))\\.(jsx?|tsx?)$"
  },
  "husky": {
    "hooks": {
      "pre-commit": "pretty-quick --staged"
    }
  }
}
